{
  "name": "awaitable-emit",
  "version": "0.0.0-dev",
  "license": "MIT",
  "keywords": [
    "nestjs",
    "kafka"
  ],
  "type": "commonjs",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "start:app": "ts-node app/main.ts",
    "test": "npm run eslint && npm run tscheck && npm run test:e2e",
    "test:r": "npm run mocha -- src/**/*.spec.ts",
    "test:w": "npm run test:r -- --watch-files src --watch",
    "lint": "npm run eslint && npm run tscheck",
    "eslint": "node node_modules/eslint/bin/eslint \"src/**/*.{ts,tsx}\"",
    "eslint:fix": "npm run eslint -- --fix",
    "tscheck": "tsc --noEmit",
    "tscheck:w": "npm run tscheck -- --watch",
    "build": "sh Taskfile buildMicrobundle4",
    "mocha": "node -r ts-node/register node_modules/mocha/bin/mocha --timeout=5s",
    "test:e2e": "npm run mocha -- app/app.e2e-spec.ts",
    "test:e2e:w": "npm run mocha -- app/app.e2e-spec.ts --watch"
  },
  "peerDependencies": {
    "@nestjs/common": "8 - 10",
    "@nestjs/microservices": "8 - 10",
    "kafkajs": "2",
    "rxjs": "7"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.17.0",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/common": "^10.4.15",
    "@nestjs/core": "^10.4.15",
    "@nestjs/microservices": "^10.4.15",
    "@nestjs/platform-express": "^10.4.15",
    "@nestjs/testing": "^10.4.15",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/express": "^5.0.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.10.2",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.19.0",
    "@typescript-eslint/parser": "^8.19.0",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.2.1",
    "expect": "^29.7.0",
    "globals": "^15.14.0",
    "kafkajs": "^2.2.4",
    "microbundle": "^0.15.1",
    "mocha": "^11.0.1",
    "prettier": "^3.4.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "semantic-release": "^24.2.0",
    "supertest": "^7.0.0",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.0"
  }
}
